{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "Buiness enterprise app that connects businesse to clients",
  "engines": {
    "node": "8.9.1"
  },
  "main": "app.js",
  "scripts": {
    "postinstall": "babel server -d distServer --presets es2015,stage-2",
    "travis-migrate": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "start": "node distServer/app.js",
    "start:dev": "nodemon server/app.js --exec babel-node --presets es2015,stage-2",
    "test": "nyc --reporter=lcov  --reporter=text  mocha --timeout 500000 --compilers js:babel-register --recursive ./server/test/",
    "tes": "mocha ./server/test/business --timeout 500000 --compilers js:babel-register --env localTest ",
    "coveralls-report": "nyc report --reporter=text-lcov | coveralls",
    "codeclimate-report": "codeclimate-test-reporter < ./coverage/lcov.info",
    "webpackdev": "webpack-dev-server --mode development",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlatunjiYso/WEconnect.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Olatunji Yusuf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OlatunjiYso/WEconnect/issues"
  },
  "homepage": "https://github.com/OlatunjiYso/WEconnect#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.4.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "sequelize-cli": "^4.0.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "alertifyjs": "^1.11.1",
    "axios": "^0.18.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-validator": "^5.0.3",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.2.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-materialize": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.36.0",
    "swagger-ui-express": "^3.0.6",
    "yamljs": "^0.3.0"
  }
}
